{
  "files.watcherExclude": {
    "**/node_modules/**": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true
  },
  "files.exclude": {
    "**/node_modules": false
  },
  "typescript.preferences.includePackageJsonAutoImports": "off",
  "editor.inlineSuggest.enabled": true
} 